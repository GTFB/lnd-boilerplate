{
  "name": "dev-agent",
  "version": "0.3.0",
  "description": "CLI assistant for automating the High-Efficiency Standard Operating Protocol.",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun run src/index.ts",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:coverage:html": "bun test --coverage --coverage-dir=coverage",
    "test:coverage:badge": "bun run src/scripts/generate-coverage-badge.ts",
    "build": "bun build src/index.ts --outdir build --target bun",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "quality": "bun run lint && bun run test",
    "quality:full": "bun run lint:fix && bun run test:coverage",
    "docs:generate": "typedoc src/index.ts --out docs/api",
    "db:init": "bun run src/scripts/init-db.ts",
    "db:reset": "bun run src/scripts/init-db.ts --reset",
    "lang:check": "bun run src/scripts/check-language.ts",
    "lang:check-staged": "bun run src/scripts/check-language.ts",
    "config:show": "bun run src/scripts/config-manager.ts show",
    "config:set": "bun run src/scripts/config-manager.ts set",
    "config:validate": "bun run src/scripts/config-manager.ts validate",
    "config:reset": "bun run src/scripts/config-manager.ts reset",
    "llm:add": "bun run src/scripts/llm-manager.ts add",
    "llm:remove": "bun run src/scripts/llm-manager.ts remove",
    "llm:list": "bun run src/scripts/llm-manager.ts list",
    "llm:test": "bun run src/scripts/llm-manager.ts test",
    "llm:default": "bun run src/scripts/llm-manager.ts default",
    "llm:update": "bun run src/scripts/llm-manager.ts update",
    "github:setup": "bun run src/scripts/github-manager.ts setup",
    "github:branches": "bun run src/scripts/github-manager.ts branches",
    "github:goals": "bun run src/scripts/github-manager.ts goals",
    "github:show": "bun run src/scripts/github-manager.ts show",
    "github:test": "bun run src/scripts/github-manager.ts test",
    "github:remove": "bun run src/scripts/github-manager.ts remove",
    "version:info": "bun run src/scripts/version-manager.ts info",
    "version:bump": "bun run src/scripts/version-manager.ts bump",
    "version:patch": "bun run src/scripts/version-manager.ts bump patch",
    "version:minor": "bun run src/scripts/version-manager.ts bump minor",
    "version:major": "bun run src/scripts/version-manager.ts bump major",
    "pre-release:alpha": "bun run src/scripts/version-manager.ts pre-release alpha",
    "pre-release:beta": "bun run src/scripts/version-manager.ts pre-release beta",
    "pre-release:rc": "bun run src/scripts/version-manager.ts pre-release rc",
    "release:prepare": "bun run src/scripts/version-manager.ts bump patch",
    "release:validate": "bun run src/scripts/validate-structure.ts && bun test"
  },
  "bin": {
    "dev": "./src/index.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^8.56.0",
    "eslint-plugin-jsdoc": "^54.1.0",
    "prettier": "^3.2.5",
    "typedoc": "^0.25.0",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "@octokit/rest": "^20.1.0",
    "commander": "^12.0.0",
    "dotenv": "^17.2.1",
    "simple-git": "^3.24.0",
    "zod": "^4.0.17"
  }
}
